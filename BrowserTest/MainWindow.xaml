        <WebBrowser x:Name="browser" Margin="10,38,10,10"/>
        <TextBox x:Name="txtUrl" Height="23" Margin="10,10,10,0" TextWrapping="Wrap" Text="http://google.com" VerticalAlignment="Top" KeyDown="txtUrl_KeyDown"/>

    </Grid>
</Window>
